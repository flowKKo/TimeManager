    <declare-styleable name="CalendarLayout">
        <attr name="default_status">
            <enum name="expand" value="0"/>
            <enum name="shrink" value="1"/>
        </attr>


        <!-- 手势模式 -->
        <attr name="gesture_mode">
            <enum name="default_mode" value="0"/>
            <!--<enum name="only_calendar" value="1" />-->
            <enum name="disabled" value="2"/>
        </attr>

        <attr name="calendar_show_mode">
            <enum name="both_month_week_view" value="0"/>
            <enum name="only_week_view" value="1"/>
            <enum name="only_month_view" value="2"/>
        </attr>
        <attr format="integer" name="calendar_content_view_id"/>
    </declare-styleable>
    <declare-styleable name="CalendarView">

        <attr format="dimension" name="calendar_padding"/><!--日历内部左右padding-->
        <attr format="dimension" name="calendar_padding_left"/><!--日历内部左padding-->
        <attr format="dimension" name="calendar_padding_right"/><!--日历内部右padding-->


        <attr format="color" name="week_background"/><!--周背景-->
        <attr format="color" name="week_line_background"/><!--线条颜色-->
        <attr format="color" name="week_text_color"/><!--周栏字体颜色-->
        <attr format="dimension" name="week_text_size"/><!--周栏字体大小-->
        <attr format="dimension" name="week_line_margin"/><!--线条margin-->

        <attr format="string" name="month_view"/><!--完全自定义月视图-->
        <attr format="string" name="week_view"/> <!--完全自定义周视图-->
        <attr format="string" name="year_view"/> <!--完全自定义年视图-->
        <attr format="dimension" name="week_bar_height"/> <!--星期栏的高度-->
        <attr format="string" name="week_bar_view"/> <!--如果需要的话使用自定义星期栏-->

        <attr format="string" name="scheme_text"/>
        <attr format="dimension" name="day_text_size"/>
        <attr format="dimension" name="lunar_text_size"/>
        <attr format="dimension" name="calendar_height"/>
        <attr format="boolean" name="calendar_match_parent"/> <!-- 全屏日历 -->
        <attr format="color" name="scheme_text_color"/>
        <attr format="color" name="scheme_month_text_color"/>
        <attr format="color" name="scheme_lunar_text_color"/>
        <attr format="color" name="scheme_theme_color"/>

        <attr format="color" name="selected_theme_color"/>
        <attr format="color" name="selected_text_color"/>
        <attr format="color" name="selected_lunar_text_color"/>

        <attr format="color" name="current_day_text_color"/>
        <attr format="color" name="current_day_lunar_text_color"/>
        <attr format="color" name="current_month_text_color"/>
        <attr format="color" name="other_month_text_color"/>
        <attr format="color" name="current_month_lunar_text_color"/>
        <attr format="color" name="other_month_lunar_text_color"/>


        <!-- 年视图相关 -->
        <attr format="dimension" name="year_view_month_text_size"/>
        <attr format="dimension" name="year_view_day_text_size"/>
        <attr format="color" name="year_view_month_text_color"/>
        <attr format="color" name="year_view_current_day_text_color"/>
        <attr format="color" name="year_view_day_text_color"/>
        <attr format="color" name="year_view_select_text_color"/>
        <attr format="color" name="year_view_scheme_color"/>
        <attr format="color" name="year_view_background"/>
        <attr format="dimension" name="year_view_month_height"/>
        <attr format="dimension" name="year_view_week_height"/>
        <attr format="dimension" name="year_view_week_text_size"/>
        <attr format="color" name="year_view_week_text_color"/>

        <attr format="dimension" name="year_view_padding"/>
        <attr format="dimension" name="year_view_padding_left"/>
        <attr format="dimension" name="year_view_padding_right"/>

        <attr format="dimension" name="year_view_month_padding_top"/>
        <attr format="dimension" name="year_view_month_padding_left"/>
        <attr format="dimension" name="year_view_month_padding_right"/>
        <attr format="dimension" name="year_view_month_padding_bottom"/>

        <!--日期范围-->
        <attr format="integer" name="min_year"/>
        <attr format="integer" name="max_year"/>
        <attr format="integer" name="min_year_month"/>
        <attr format="integer" name="max_year_month"/>
        <attr format="integer" name="min_year_day"/>
        <attr format="integer" name="max_year_day"/>


        <!--月视图是否可滚动-->
        <attr format="boolean" name="month_view_scrollable"/>
        <!--周视图是否可滚动-->
        <attr format="boolean" name="week_view_scrollable"/>
        <!--年视图是否可滚动-->
        <attr format="boolean" name="year_view_scrollable"/>

        <!-- 月份显示模式 -->
        <attr name="month_view_show_mode">
            <enum name="mode_all" value="0"/>
            <enum name="mode_only_current" value="1"/>
            <enum name="mode_fix" value="2"/>
        </attr>

        <!-- 自定义周起始 -->
        <attr name="week_start_with">
            <enum name="sun" value="1"/>
            <enum name="mon" value="2"/>
            <enum name="sat" value="7"/>
        </attr>

        <!-- 自定义选择模式 -->
        <attr name="select_mode">
            <enum name="default_mode" value="0"/>
            <enum name="single_mode" value="1"/>
            <enum name="range_mode" value="2"/>
            <enum name="multi_mode" value="3"/>
        </attr>

        <!-- when select_mode = multi_mode -->
        <attr format="integer" name="max_multi_select_size"/>

        <!-- when select_mode = range_mode -->
        <attr format="integer" name="min_select_range"/>
        <attr format="integer" name="max_select_range"/>

        <!-- auto select day -->
        <attr name="month_view_auto_select_day">
            <enum name="first_day_of_month" value="0"/>
            <enum name="last_select_day" value="1"/>
            <enum name="last_select_day_ignore_current" value="2"/>
        </attr>
    </declare-styleable>
</resources>